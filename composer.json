{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "7.4.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/assert": "^3.2",
        "brentybh/bing-wallpaper": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "ipip/db": "^1.0",
        "league/uri": "^6.0",
        "league/uri-components": "^2.1",
        "leancloud/leancloud-sdk": "^0.10.3",
        "maxmind-db/reader": "^1.5",
        "spatie/data-transfer-object": "^1.13",
        "spomky-labs/base64url": "^2.0",
        "spomky-labs/otphp": "^10.0",
        "symfony/console": "*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-intl-messageformatter": "^1.14",
        "symfony/translation": "5.0.*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*",
        "thecodingmachine/safe": "^1.0"
    },
    "suggest": {
        "pcrov/jsonreader": "JSON reader"
    },
    "require-dev": {
        "sensiolabs/security-checker": "^6.0",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.13"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {},
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "repositories": {
        "0": {
            "type": "vcs",
            "url": "git@e.coding.net:brent/bing-wallpaper.git"
        },
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
